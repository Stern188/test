{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "ng-cloud"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "env-vars",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../node_modules/material-design-icons/iconfont/material-icons.css",
        "../node_modules/prismjs/themes/prism-dark.css",
        "../node_modules/select2/dist/css/select2.css",
        "../node_modules/bootstrap-material-design/dist/css/ripples.min.css",
        "../node_modules/vex-js/dist/css/vex.css",
        "../node_modules/vex-js/dist/css/vex-theme-default.css",
        "styles.scss",
        "../node_modules/bootstrap/dist/css/bootstrap.css",
        "../src/assets/plugins/powerfloat/powerFloat.css",
        "../src/assets/plugins/powerfloat/common.css",
        "../src/assets/plugins/laydate/laydate.css"
      ],
      "stylePreprocessorOptions": {
        "includePaths": [
          "../node_modules/bootstrap-sass/assets/stylesheets"
        ]
      },
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/prismjs/prism.js",
        "../node_modules/arrive/minified/arrive.min.js",
        "../node_modules/bootstrap-material-design/dist/js/material.js",
        "../node_modules/bootstrap-material-design/dist/js/ripples.min.js",
        "../node_modules/selectize/dist/js/standalone/selectize.js",
        "../node_modules/echarts/dist/echarts.min.js",
        "../node_modules/select2/dist/js/select2.js",
        "../node_modules/nouislider/distribute/nouislider.js",
        "../src/assets/plugins/powerfloat/jquery-powerFloat.js",
        "../node_modules/bootstrap/dist/js/bootstrap.js",
        "../src/assets/plugins/laydate/laydate.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}